        <item>
        <title>Gallery</title>
        <link>http://localhost:1313/gallery/pictures/</link>
        <pubDate>Tue, 16 Jul 2024 11:26:54 +0200</pubDate>
        
        <guid>http://localhost:1313/gallery/pictures/</guid>
        <description>&lt;p&gt;Here are some of my favorite pictures I&amp;rsquo;ve taken!&lt;/p&gt;
&lt;h2 id=&#34;color&#34;&gt;Color
&lt;/h2&gt;


&lt;style&gt;
  .image-grid {
    display: grid;
    grid-template-columns: repeat(3, 1fr);  
    gap: 10
  }
  .image-grid img {
    height: 200px;  
    object-fit: cover;
    cursor: pointer;
  }
  .modal {
    display: none; 
    position: fixed; 
    z-index: 1000; 
    left: 0; 
    top: 0; 
    width: 100%; 
    height: 100%; 
    overflow: auto; 
    background-color: rgba(0,0,0,0.8); 
  }
  .modal-content {
    margin: 5% auto; 
    display: block; 
    width: 80%; 
    max-width: 700px; 
  }
  .close {
    position: absolute; 
    top: 15px; 
    right: 35px; 
    color: #fff; 
    font-size: 40px; 
    font-weight: bold; 
    cursor: pointer; 
  }
  .close:hover,
  .close:focus {
    color: #bbb; 
    text-decoration: none; 
    cursor: pointer; 
  }
&lt;/style&gt;

&lt;div class=&#34;image-grid&#34;&gt;

  
    &lt;img src=&#34;http://localhost:1313/color/Corner%20stone%20inverted.jpg&#34; alt=&#34;Corner stone inverted.jpg&#34; class=&#34;image&#34;&gt;
  

  
    &lt;img src=&#34;http://localhost:1313/color/HONDA.jpg&#34; alt=&#34;HONDA.jpg&#34; class=&#34;image&#34;&gt;
  

  
    &lt;img src=&#34;http://localhost:1313/color/Huisje%20in%20Riven.jpg&#34; alt=&#34;Huisje in Riven.jpg&#34; class=&#34;image&#34;&gt;
  

  
    &lt;img src=&#34;http://localhost:1313/color/Old%20Red%20lights.jpg&#34; alt=&#34;Old Red lights.jpg&#34; class=&#34;image&#34;&gt;
  

  
    &lt;img src=&#34;http://localhost:1313/color/Retro%20gebouw.jpg&#34; alt=&#34;Retro gebouw.jpg&#34; class=&#34;image&#34;&gt;
  

  
    &lt;img src=&#34;http://localhost:1313/color/Station%20after%20dark.jpg&#34; alt=&#34;Station after dark.jpg&#34; class=&#34;image&#34;&gt;
  

&lt;/div&gt;


&lt;div id=&#34;myModal&#34; class=&#34;modal&#34;&gt;
  &lt;span class=&#34;close&#34;&gt;&amp;times;&lt;/span&gt;
  &lt;img class=&#34;modal-content&#34; id=&#34;modalImage&#34;&gt;
&lt;/div&gt;

&lt;script&gt;
  document.addEventListener(&#34;DOMContentLoaded&#34;, function() {
    const images = document.querySelectorAll(&#39;.image-grid .image&#39;);
    const modal = document.getElementById(&#39;myModal&#39;);
    const modalImg = document.getElementById(&#39;modalImage&#39;);
    const closeBtn = document.querySelector(&#39;.modal .close&#39;);

    images.forEach(img =&gt; {
      img.onclick = function() {
        modal.style.display = &#39;block&#39;;
        modalImg.src = this.getAttribute(&#39;data-src&#39;);
      }
    });

    closeBtn.onclick = function() {
      modal.style.display = &#39;none&#39;;
    }

    window.onclick = function(event) {
      if (event.target == modal) {
        modal.style.display = &#39;none&#39;;
      }
    }
  });
&lt;/script&gt;

